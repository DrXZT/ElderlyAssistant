<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.asciimovie.drxzt.elderlyassistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.user.FoodBaconActivity"></activity>
        <activity android:name=".activity.user.FoodPineappleActivity" />
        <activity android:name=".activity.user.FoodPotatoActivity" />
        <activity android:name=".activity.user.FoodShrimpActivity" />
        <activity android:name=".activity.user.FoodSpinachActivity" />
        <activity android:name=".activity.user.FestivalsActivity" />
        <activity android:name=".activity.user.ServiceActivity" />
        <activity android:name=".activity.user.SaleActivity" />
        <activity android:name=".activity.user.VolunteerActivity" />
        <activity android:name=".activity.user.FoodActivity" />
        <activity android:name=".activity.user.CommunityActivity" />
        <activity android:name=".activity.user.LoginActivity" />
        <activity android:name=".activity.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>